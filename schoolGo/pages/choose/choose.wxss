view {
    padding: 10rpx 20rpx;
}

.notice {
    font-size: 50rpx;
    font-weight: 600;
    padding-top: 30rpx;
}

.pageContainer {
    display: flex;
    justify-content: space-around;
    align-items: center;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 95vw;
    height: 100rpx;
}

.item {
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background-color: rgb(94, 205, 50);
    border-radius: 20rpx;
}